//          *
//         * *
//		    * * *
//	     * * * *
//	    * * * * * 
//	     * * * * 
//	      * * * 
//		     * * 
//		      * 

#include <stdio.h>
int main(){
	for(int i = 6;i>=1;i--){
		for(int j = 1;j<i;j++){
			printf(" ");}
		for(int k = 1;k<=6-i;k++){
			printf("* ");}
		printf("\n");}
	
	for(int p = 6;p>=1;p--){
		for(int r = 0;r<=6-p;r++){
			printf(" ");}
		for(int q = 1;q<p-1;q++){
			printf("* ");}
		printf("\n");}
	}
	
	
	
